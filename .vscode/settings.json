{
    "python.pythonPath": "${workspaceFolder}\\venv\\Scripts\\python.exe",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true
    },
    "editor.rulers": [
        100
    ],
    "editor.minimap.enabled": false,
    "editor.detectIndentation": false
}